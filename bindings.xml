<Bindings>
	<Binding name="Toggle Targets +" header="RETARDED" category="ADDONS">
		Retarded_ToggleTargetMode()
	</Binding>
	<Binding name="Toggle Targets -" category="ADDONS">
		Retarded_ToggleTargetModeReverse()
	</Binding>
	<Binding name="Set Targets to 1" category="ADDONS">
		Retarded_SetTargetMode(1)
	</Binding>
	<Binding name="Set Targets to 2" category="ADDONS">
		Retarded_SetTargetMode(2)
	</Binding>
	<Binding name="Set Targets to 3" category="ADDONS">
		Retarded_SetTargetMode(3)
	</Binding>
	<Binding name="Set Targets to 4+" category="ADDONS">
		Retarded_SetTargetMode(4)
	</Binding>
</Bindings>
