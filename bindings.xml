	</Binding>
	<Binding name="RETARDED_TARGET2" category="BINDING_CATEGORY_RETARDED">
		Retarded_SetTargetMode(2)
	</Binding>
	<Binding name="RETARDED_TARGET3" category="BINDING_CATEGORY_RETARDED">
		Retarded_SetTargetMode(3)
	</Binding>
	<Binding name="RETARDED_TARGET4" category="BINDING_CATEGORY_RETARDED">
		Retarded_SetTargetMode(4)
	</Binding>
	<Binding name="RETARDED_TARGET5" category="BINDING_CATEGORY_RETARDED">
